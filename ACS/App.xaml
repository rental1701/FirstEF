<Application x:Class="ACS.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ACS"
             xmlns:con="clr-namespace:ACS.Infrastructure.Converters"
             StartupUri="View/MainWindow.xaml">
    
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <con:VisabilityConverterUI x:Key="VisConvUI"/>
                    <con:UriConverter x:Key="UriConv"/>
                </ResourceDictionary>
                <ResourceDictionary x:Name="btnUpdate" Source="Resources\Style\ButtonStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
